{
	"name": "dav-npm",
	"version": "0.2.1",
	"description": "This is the npm package for accessing the dav API",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "./node_modules/mocha/bin/mocha -r mock-local-storage -r ts-node/register test/**/*.spec.ts",
		"prepublishOnly": "npm run build",
		"compile": "npm run clean && tsc -p . && npm run copyJs",
		"watch": "tsc -w -p .",
		"build": "npm run compile",
		"prepare": "npm run compile",
		"clean": "rm -rf dist",
		"copyJs": "cpx lib/workers/*.js dist/lib/workers"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Dav2070/dav-npm.git"
	},
	"author": "Dav",
	"license": "MIT",
	"devDependencies": {
		"@types/chai": "^4.1.4",
		"@types/mocha": "^5.2.3",
		"@types/node": "^8.0.0",
		"chai": "^4.1.2",
		"cpx": "^1.5.0",
		"mocha": "^5.2.0",
		"mock-local-storage": "^1.0.5",
		"ts-node": "^7.0.0",
		"typescript": "^2.6.1"
	},
	"bugs": {
		"url": "https://github.com/Dav2070/dav-npm/issues"
	},
	"homepage": "https://github.com/Dav2070/dav-npm",
	"files": [
		"dist/index.js",
		"dist/index.d.ts",
		"dist/lib"
	],
	"dependencies": {
		"@types/platform": "^1.3.1",
		"axios": "^0.18.0",
		"bowser": "^1.9.3",
		"localforage": "^1.7.1",
		"platform": "^1.3.5",
		"rxjs": "^6.2.0"
	}
}
